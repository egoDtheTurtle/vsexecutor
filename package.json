{
  "name": "vsexecutor",
  "displayName": "VSExecutor",
  "description": "Execute lego lua with vscode button.",
  "version": "0.0.3",
  "publisher": "egoDtheTurtle",
  "repository": {
    "type": "git",
    "url": "https://github.com/egoDtheTurtle/VSExecutor"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint .",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "watch": "tsc -w"
  },
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:lua"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.executeFile",
        "title": "Execute Lua Script"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "executeLuaFile",
          "name": "Execute Lua Script"
        }
      ]
    }
  },
  "dependencies": {
    "ws": "^8.0.0"
  },
  "devDependencies": {
    "vscode": "^1.1.37",
    "@types/vscode": "^1.95.0",
    "@types/ws": "^8.5.13",
    "typescript": "^4.9.5",
    "vsce": "^1.97.0"
  }
}
